# === DELIVERY ===

channels:
  - id: ch-website
    name: Website & SEO
    type: owned
    segments: [cs-startups, cs-freelancers]
    phases: [awareness, evaluation, purchase]

  - id: ch-content
    name: Content Marketing
    type: owned
    segments: [cs-startups, cs-freelancers, cs-agencies]
    phases: [awareness, evaluation]

  - id: ch-enterprise-sales
    name: Enterprise Sales Team
    type: direct
    segments: [cs-enterprise]
    phases: [evaluation, purchase]

  - id: ch-app-stores
    name: App Stores
    type: indirect
    segments: [cs-freelancers]
    phases: [awareness, purchase]

  - id: ch-referrals
    name: Referral Program
    type: owned
    segments: [cs-startups, cs-freelancers, cs-agencies]
    phases: [awareness]

  - id: ch-platform
    name: Platform Experience
    type: owned
    segments: [cs-startups, cs-enterprise, cs-freelancers, cs-agencies]
    phases: [delivery, after_sales]

  - id: ch-partner-agencies
    name: Partner Agencies
    type: partner
    segments: [cs-startups]
    phases: [awareness, purchase]

customer_relationships:
  - id: cr-self-service-startups
    segment: cs-startups
    type: self_service
    description: Self-service with help center, chat support, and community forums

  - id: cr-dedicated-enterprise
    segment: cs-enterprise
    type: dedicated
    description: Dedicated account manager, success team, and premium SLA

  - id: cr-community-freelancers
    segment: cs-freelancers
    type: community
    description: Community forums, webinars, and peer mentorship programs

  - id: cr-self-service-agencies
    segment: cs-agencies
    type: self_service
    description: Self-service with partner resources and priority support

# === FINANCES ===

revenue_streams:
  - id: rs-client-fees
    name: Client Service Fees
    type: transaction
    from_segments: [cs-startups]
    for_value: vp-client-platform
    pricing: fixed_menu

  - id: rs-enterprise-subscription
    name: Enterprise Subscription
    type: subscription
    from_segments: [cs-enterprise]
    for_value: vp-enterprise-platform
    pricing: negotiation

  - id: rs-freelancer-fees
    name: Freelancer Service Fees
    type: transaction
    from_segments: [cs-freelancers]
    for_value: vp-freelancer-platform
    pricing: fixed_menu

  - id: rs-agency-fees
    name: Agency Partner Fees
    type: subscription
    from_segments: [cs-agencies]
    for_value: vp-agency-tools
    pricing: fixed_menu

  - id: rs-premium-features
    name: Premium Freelancer Features
    type: subscription
    from_segments: [cs-freelancers]
    for_value: vp-freelancer-platform
    pricing: fixed_menu

cost_structure:
  type: value_driven
  characteristics: [fixed_costs_heavy, economies_of_scale]
  major_costs:
    - name: Engineering & Product
      type: fixed
      linked_to: [kr-engineering, ka-platform-dev]
    - name: Trust & Safety
      type: fixed
      linked_to: [kr-trust-team, ka-trust-safety]
    - name: Sales & Marketing
      type: variable
      linked_to: [ka-growth, ka-enterprise-sales]
    - name: Payment Processing
      type: variable
      linked_to: [kr-payment-infrastructure]
    - name: Customer Support
      type: variable
      linked_to: [ka-support]

# === INFRASTRUCTURE ===

key_resources:
  - id: kr-talent-network
    name: Freelancer Network
    type: human
    description: Global network of vetted freelancers across skill categories
    for_value: [vp-client-platform, vp-enterprise-platform, vp-agency-tools]

  - id: kr-engineering
    name: Engineering Team
    type: human
    description: Product engineers, data scientists, and platform architects
    for_value: [vp-client-platform, vp-enterprise-platform, vp-freelancer-platform, vp-agency-tools]

  - id: kr-trust-team
    name: Trust & Safety Team
    type: human
    description: Vetting specialists, fraud prevention, and dispute mediators
    for_value: [vp-client-platform, vp-freelancer-platform]

  - id: kr-platform
    name: Platform Technology
    type: intellectual
    description: Matching algorithms, compliance engine, and platform infrastructure
    for_value: [vp-client-platform, vp-enterprise-platform, vp-freelancer-platform, vp-agency-tools]

  - id: kr-payment-infrastructure
    name: Payment Infrastructure
    type: financial
    description: Escrow system, multi-currency support, and payout rails
    for_value: [vp-client-platform, vp-freelancer-platform]

  - id: kr-brand
    name: Brand & Reputation
    type: intellectual
    description: Trusted brand known for quality talent and fair practices
    for_value: [vp-client-platform, vp-enterprise-platform, vp-freelancer-platform]

  - id: kr-data
    name: Marketplace Data
    type: intellectual
    description: Pricing benchmarks, skill trends, and matching optimization data
    for_value: [vp-client-platform, vp-enterprise-platform]

key_activities:
  - id: ka-platform-dev
    name: Platform Development
    type: platform
    description: Building and improving marketplace platform and matching algorithms
    for_value: [vp-client-platform, vp-enterprise-platform, vp-freelancer-platform, vp-agency-tools]

  - id: ka-trust-safety
    name: Trust & Safety Operations
    type: problem_solving
    description: Vetting freelancers, monitoring quality, handling disputes
    for_value: [vp-client-platform, vp-freelancer-platform]

  - id: ka-growth
    name: Growth Marketing
    type: problem_solving
    description: Acquiring freelancers and clients to maintain marketplace liquidity
    for_value: [vp-client-platform, vp-freelancer-platform]

  - id: ka-enterprise-sales
    name: Enterprise Sales
    type: problem_solving
    description: Solution selling and account management for enterprise clients
    for_value: [vp-enterprise-platform]

  - id: ka-support
    name: Customer Support
    type: problem_solving
    description: Supporting clients, freelancers, and agencies across the platform
    for_value: [vp-client-platform, vp-freelancer-platform, vp-agency-tools]

  - id: ka-community
    name: Community Management
    type: problem_solving
    description: Building freelancer community through content, events, and forums
    for_value: [vp-freelancer-platform]

key_partnerships:
  - id: kp-payment-processors
    name: Payment Processors
    type: strategic_alliance
    motivation: optimization
    provides: [kr-payment-infrastructure]

  - id: kp-background-check
    name: Background Check Providers
    type: supplier
    motivation: resource_acquisition
    provides: [ka-trust-safety]

  - id: kp-skills-assessment
    name: Skills Assessment Partners
    type: strategic_alliance
    motivation: resource_acquisition
    provides: [ka-trust-safety]

  - id: kp-cloud-providers
    name: Cloud Infrastructure
    type: supplier
    motivation: optimization
    provides: [kr-platform]

  - id: kp-staffing-integrations
    name: Enterprise HR Integrations
    type: strategic_alliance
    motivation: resource_acquisition
    provides: [ka-enterprise-sales]
