        - vp-convenience
        - vp-culinary-experience
  - id: kr-culinary-team
    name: Culinary Team
    for:
      value_propositions:
        - vp-culinary-experience
  - id: kr-fulfillment-center
    name: Fulfillment Centers
    for:
      value_propositions:
