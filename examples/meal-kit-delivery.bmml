        - vp-culinary-experience
  - id: ka-personalization
    name: Personalization Engine
    for:
      value_propositions:
        - vp-convenience
key_partnerships:
  - id: kp-farms
    name: Local Farms & Producers
    for:
      key_resources:
        - kr-supply-chain
  - id: kp-delivery
    name: Delivery Partners
    for:
      key_activities:
        - ka-fulfillment
  - id: kp-packaging
    name: Sustainable Packaging Supplier
    for:
