  - id: ka-user-acquisition
    name: Free User Acquisition
    for:
      value_propositions: [vp-free-storage]

  - id: ka-conversion-optimization
    name: Conversion Optimization & A/B Testing
    for:
      value_propositions: [vp-premium-storage]

  - id: ka-customer-support
    name: Customer Support Operations
    for:
      value_propositions: [vp-free-storage, vp-premium-storage]

# === KEY PARTNERSHIPS ===

