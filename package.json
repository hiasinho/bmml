{
  "name": "bmml",
  "version": "0.1.0",
  "description": "A markup format for describing business models, based on Alexander Osterwalder's work",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "bmml": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit",
    "validate": "node --import tsx ./src/cli.ts validate"
  },
  "keywords": [
    "business-model",
    "business-model-canvas",
    "value-proposition-canvas",
    "canvas",
    "osterwalder",
    "yaml",
    "validation",
    "markup-language"
  ],
  "author": "Mathias Maisberger",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiasinho/bmml.git"
  },
  "homepage": "https://hiasinho.github.io/bmml/",
  "bugs": {
    "url": "https://github.com/hiasinho/bmml/issues"
  },
  "files": [
    "dist",
    "schemas",
    "README.md",
    "LICENSE.md"
  ],
  "packageManager": "pnpm@10.28.1",
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.9",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
