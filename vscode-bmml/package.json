{
  "name": "bmml",
  "displayName": "BMML",
  "description": "Language support for BMML (Business Model Markup Language) files (.bmml)",
  "version": "0.1.0",
  "publisher": "bmml",
  "repository": {
    "type": "git",
    "url": "https://github.com/bmclang/bmclang"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "bmml",
        "aliases": ["BMML", "bmml", "Business Model Markup Language"],
        "extensions": [".bmml"],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/bmml.svg",
          "dark": "./icons/bmml.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "bmml",
        "scopeName": "source.bmml",
        "path": "./syntaxes/bmml.tmLanguage.json"
      }
    ],
    "yamlValidation": [
      {
        "fileMatch": "*.bmml",
        "url": "./schemas/bmclang.schema.json"
      }
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/vscode": "^1.75.0"
  }
}
